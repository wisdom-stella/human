<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="space.estrrella.ico3.Car" id="car">
		<property name="name" value="아우디"/>
	</bean>
	<bean class="space.estrrella.ico3.Car" id="car1"> <!-- Car car1 = new Car1(); car1.setName("아우디1"); -->
		<property name="name" value="아우디1"/>
	</bean>
	<bean class="space.estrrella.ico3.Car" id="car2">
		<constructor-arg>
			<ref bean="str"/>
		</constructor-arg>
	</bean>
	<bean class="space.estrrella.ico3.Audi" id="audi"> <!-- String str = "abcd"; String str = new String("abcd"); -->
		<property name="car" ref="car"/>
	</bean>
	<bean class="java.lang.String" id="str">
		<constructor-arg value="abcd"/>
	</bean>
	<bean class="java.lang.Integer" id="iii">
		<constructor-arg>
			<value>10</value>
		</constructor-arg>
	</bean>
</beans>
